<%= form_for @notum, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :nro, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :nro, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :letra, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :letra, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                nota_path, :class => 'btn' %>
  </div>
<% end %>
