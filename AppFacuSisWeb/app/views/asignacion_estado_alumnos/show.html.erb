<%- model_class = AsignacionEstadoAlumno -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @asignacion_estado_alumno.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:alumno_id) %>:</strong></dt>
  <dd><%= @asignacion_estado_alumno.alumno_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:semestre_id) %>:</strong></dt>
  <dd><%= @asignacion_estado_alumno.semestre_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:periodo_lectivo_id) %>:</strong></dt>
  <dd><%= @asignacion_estado_alumno.periodo_lectivo_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:materia_id) %>:</strong></dt>
  <dd><%= @asignacion_estado_alumno.materia_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sede_id) %>:</strong></dt>
  <dd><%= @asignacion_estado_alumno.sede_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estado_alumno_id) %>:</strong></dt>
  <dd><%= @asignacion_estado_alumno.estado_alumno_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              asignacion_estado_alumnos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_asignacion_estado_alumno_path(@asignacion_estado_alumno), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              asignacion_estado_alumno_path(@asignacion_estado_alumno),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
